{
	"name":"ViewController",
	"root":{
		"properties":{
			"backgroundColor":"white"
		},
		"views":{
			"!imageHolder:UIImageView":{
				"constraints":{
					"width height":"+160",
					"top":"@super +34",
					"centerX":"@super"
				},
				"properties":{
					"image":"https://res.cloudinary.com/teepublic/image/private/s--PzP3ZyfY--/t_Preview/b_rgb:ffffff,c_limit,f_jpg,h_630,q_90,w_630/v1457761519/production/designs/445119_1.jpg",
					"contentMode":"scaleAspectFit",
					"clipToBounds":"true"
				}
			},
			"!labelUsername:UILabel":{
				"constraints":{
					"left":"@super.leftMargin",
					"right":"@super.rightMargin",
					"top":"@imageHolder.bottom +48"
				},
				"properties":{
					"text":"Username",
					"textColor":"#333333"
				}
			},
			"inputUsername":{
				"constraints":{
					"left right":"@labelUsername",
					"height":"+50",
					"top":"@labelUsername.bottom +8"
				},
				"properties":{
					"backgroundColor":"#f7fbfc"
				}
			},
			"!labelPassword:UILabel":{
				"constraints":{
					"left right":"@labelUsername",
					"top":"@inputUsername.bottom +16"
				},
				"properties":{
					"text":"Password",
					"textColor":"#333333"
				}
			},
			"inputPassword":{
				"constraints":{
					"left right height":"@inputUsername",
					"top":"@labelPassword.bottom +8"
				},
				"properties":{
					"backgroundColor":"#f7fbfc"
				}
			},
			"!buttonWrapper":{
				"constraints":{
					"left right":"@super",
					"top":"@inputPassword.bottom +24"
				},
				"views":{
					"!leftButton":{
						"constraints":{
							"top left bottom":"@super",
							"width":"@super /2"
						},
						"views":{
							"buttonLogin":{
								"constraints":{
									"center top bottom":"@super",
									"width":"%>= +120",
									"height":"%>= +50"
								},
								"properties":{
									"backgroundColor":"#333333",
									"textColor":"white",
									"text":"Login",
									"cornerRadius":"10"
								}
							}
						}
					},
					"!rigthButton":{
						"constraints":{
							"top right bottom":"@super",
							"left":"@leftButton.right"
						},
						"views":{
							"buttonRegister":{
								"constraints":{
									"width height":"@buttonLogin",
									"center":"@super"
								},
								"properties":{
									"backgroundColor":"orange",
									"textColor":"white",
									"text":"Register!",
									"cornerRadius":"10"
								}
							}
						}
					}
				}
			}
		}
	}
}